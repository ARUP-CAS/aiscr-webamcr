      <xsd:element name="datum_zapisu_zahajeni" minOccurs="0" maxOccurs="1" type="xsd:date"/> <!-- moved to projekt/historie/datum_zmeny; represented by records with typ_zmeny="P23" -->
      <xsd:element name="odpovedny_pracovnik_zahajeni" minOccurs="0" maxOccurs="1" type="xsd:string"/> <!-- moved to projekt/historie/uzivatel; represented by records with typ_zmeny="P23" -->
