<?xml version="1.0" encoding="utf-8" ?>
<!-- This is an annotated version of the AMCR XML schema to highlight changes (v1.2 -> v2.0) in the the structure of the schema used by the AMCR OAI-PMH API. -->

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:oai_amcr="https://api.aiscr.cz/schema/oai_amcr" targetNamespace="https://api.aiscr.cz/schema/oai_amcr" elementFormDefault="unqualified" version="1.2">

<!-- elementFormDefault was changed to "qualified" so every element in metadata part of the OAI-PMH response is now prefixed with namespace; the namespace "oai_amcr" was shortened to "amcr" as well as XSD filename -->

  <xsd:annotation>
    <xsd:documentation>XML Schema for native export of the AMCR database (version 1.2). Related API: https://api.aiscr.cz/</xsd:documentation>
  </xsd:annotation>

  <xsd:element name="amcr">
    <xsd:complexType>
      <xsd:choice>
        <xsd:element name="projekt" type="oai_amcr:ProjectType"/>
        <xsd:element name="akce" type="oai_amcr:EventType"/> <!-- merged into archeologicky_zaznam -->
        <xsd:element name="lokalita" type="oai_amcr:SiteType"/> <!-- merged into archeologicky_zaznam -->
        <xsd:element name="let" type="oai_amcr:FlightType"/>
        <xsd:element name="dok_jednotka" type="oai_amcr:DocumentationUnitType"/> <!-- merged into archeologicky_zaznam/dokumentacni_jednotka -->
        <xsd:element name="dokument" type="oai_amcr:DocumentType"/>
        <xsd:element name="soubor" type="oai_amcr:FileType"/> <!-- merged into specific parrent data class - projekt/dokument/samostatny_nalez -->
        <xsd:element name="ext_zdroj" type="oai_amcr:ExternalSourceType"/>
        <xsd:element name="pian" type="oai_amcr:PianType"/>
        <xsd:element name="adb" type="oai_amcr:AdbType"/>
        <xsd:element name="samostatny_nalez" type="oai_amcr:IndividualFindType"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>

<!-- All elements refering to other records provided by the AMCR API now contain an "id" attribute with the string value which can be used to harvest the referred record. To get the full OAI-PMH record identifier, the id string have to be merged with a prefix. The structure of the prefix is [AMCR API base URL]/id/[id string] (e.g.: https://api.aiscr.cz/id/C-TX-202300073). However, some records may not be accessible to all users. The reference is available not only for child and parent records, but now also for all vocabulary terms (i.e. types, cadastres, persons, users, organisations, etc.). It is advised to store the id string alongside the label as it is guaranteed to be persistent and the label itself may be subject to updates. -->

