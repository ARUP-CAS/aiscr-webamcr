      <xsd:element name="dalsi_katastry" minOccurs="0" maxOccurs="1" type="xsd:string"/> <!-- moved to projekt/chranene_udaje/dalsi_katastr; each item is now listed as separate element, without using multivalues with ";" delimiter -->
